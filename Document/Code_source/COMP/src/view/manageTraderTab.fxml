<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="750.0" prefWidth="1100.0"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.ManageTraderTabController">
	<children>
		<VBox alignment="TOP_CENTER" prefHeight="750.0" prefWidth="350.0"
			spacing="5.0">
			<padding>
				<Insets left="10.0" right="10.0" top="30.0" />
			</padding>
			<children>
				<Label text="[거래처 정보]">
					<font>
						<Font size="30.0" />
					</font>
					<VBox.margin>
						<Insets />
					</VBox.margin>
					<padding>
						<Insets bottom="20.0" left="10.0" top="10.0" />
					</padding>
				</Label>
				<VBox alignment="CENTER" prefHeight="157.0" prefWidth="330.0"
					spacing="10.0">
					<children>
						<HBox alignment="CENTER" prefHeight="40.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="거래처번호" />
								<TextField fx:id="txtTRNum" prefWidth="160.0" />
							</children>
						</HBox>
						<HBox alignment="CENTER" prefHeight="40.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="거래처명" />
								<TextField fx:id="txtTRName" prefWidth="160.0" />
							</children>
						</HBox>
					</children>
				</VBox>
				<VBox alignment="CENTER" prefHeight="160.0" prefWidth="330.0"
					spacing="20.0">
					<children>
						<HBox alignment="CENTER" prefHeight="30.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="전화번호" />
								<TextField fx:id="txtTRPhone" prefWidth="160.0" />
							</children>
						</HBox>
						<HBox alignment="CENTER" prefHeight="30.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="주소" />
								<TextField fx:id="txtTRAddress" prefWidth="160.0" />
							</children>
						</HBox>
					</children>
				</VBox>
				<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
					spacing="20.0">
					<children>
						<HBox alignment="CENTER" prefHeight="30.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="계좌주" />
								<TextField fx:id="txtTRBOwner" prefWidth="160.0" />
							</children>
						</HBox>
						<HBox alignment="CENTER" prefHeight="30.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="계좌번호" />
								<TextField fx:id="txtTRBNum" prefWidth="160.0" />
							</children>
						</HBox>
						<HBox alignment="CENTER" prefHeight="30.0" prefWidth="280.0"
							spacing="20.0">
							<children>
								<Label alignment="CENTER" prefWidth="60.0" text="계좌은행" />
								<TextField fx:id="txtTRBName" prefWidth="160.0" />
							</children>
						</HBox>
					</children>
				</VBox>
				<HBox alignment="CENTER" prefHeight="200.0" prefWidth="330.0"
					spacing="20.0">
					<children>
						<Button fx:id="btnTRInsert" mnemonicParsing="false"
							onAction="#btnTRInsert" prefHeight="30.0" text="제품 등록" />
						<Button fx:id="btnTRUpdate" mnemonicParsing="false"
							onAction="#btnTRUpdate" prefHeight="30.0" text="제품 수정" />
						<Button fx:id="btnTRDelete" mnemonicParsing="false"
							onAction="#btnTRDelete" prefHeight="30.0" text="제품 삭제" />
					</children>
					<padding>
						<Insets bottom="30.0" />
					</padding>
				</HBox>
			</children>
		</VBox>
		<VBox prefHeight="750.0" prefWidth="750.0">
			<children>
				<HBox alignment="BOTTOM_LEFT" prefHeight="100.0"
					prefWidth="200.0">
					<padding>
						<Insets bottom="10.0" right="200.0" />
					</padding>
					<children>
						<ComboBox fx:id="cmbTRSearchKey" prefWidth="130.0"
							promptText="검색 구분" />
						<TextField fx:id="txtTRSearchValue" prefWidth="310.0" />
						<Button fx:id="btnTRSearch" mnemonicParsing="false"
							onAction="#btnTRSearch" prefWidth="50.0" text="검색" />
					</children>
				</HBox>
				<TableView fx:id="traderTableView"
					onMouseClicked="#traderTableView" prefHeight="600.0"
					prefWidth="690.0">
					<columns>
						<TableColumn prefWidth="75.0" text="거래처번호" />
						<TableColumn prefWidth="75.0" text="거래처명" />
						<TableColumn prefWidth="75.0" text="전화번호" />
						<TableColumn prefWidth="75.0" text="주소" />
						<TableColumn prefWidth="119.0" text="계좌주" />
						<TableColumn prefWidth="139.0" text="계좌번호" />
						<TableColumn prefWidth="131.0" text="계좌은행" />
						<TableColumn prefWidth="131.0" text="등록일" />
					</columns>
				</TableView>
			</children>
			<padding>
				<Insets bottom="50.0" left="10.0" right="50.0" top="50.0" />
			</padding>
		</VBox>
	</children>
</HBox>

