							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.rowIndex="3" />
						<ImageView fx:id="productImageView13" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="1"
							GridPane.rowIndex="3" />
						<ImageView fx:id="productImageView23" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="2"
							GridPane.rowIndex="3" />
						<ImageView fx:id="productImageView33" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="3"
							GridPane.rowIndex="3" />
