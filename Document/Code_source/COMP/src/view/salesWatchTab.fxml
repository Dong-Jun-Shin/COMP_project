<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="750.0" prefWidth="1100.0"
	spacing="10.0" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.SalesWatchTabController">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<children>
		<TreeView fx:id="productTreeView" prefHeight="730.0"
			prefWidth="250.0" />
		<VBox prefHeight="730.0" prefWidth="850.0">
			<children>
				<GridPane gridLinesVisible="true" prefHeight="730.0"
					prefWidth="820.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<ImageView fx:id="productImageView00" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" />
						<ImageView fx:id="productImageView10" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="1" />
						<ImageView fx:id="productImageView20" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="2" />
						<ImageView fx:id="productImageView30" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="3" />
						<ImageView fx:id="productImageView01" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView11" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="1"
							GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView21" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="2"
							GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView31" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="3"
							GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView02" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.rowIndex="2" />
						<ImageView fx:id="productImageView12" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="1"
							GridPane.rowIndex="2" />
						<ImageView fx:id="productImageView22" fitHeight="150.0"
							fitWidth="200.0" onMouseClicked="#imageSel" pickOnBounds="true"
							preserveRatio="true" GridPane.columnIndex="2"
							GridPane.rowIndex="2" />
